<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>SGV</title>

    @Styles.Render("~/Content/master/estilos")
    @Styles.Render("~/bundles/Assets/css")
    @Scripts.Render("~/bundles/jquery/js")
    @Scripts.Render("~/bundles/angular/js")

     
</head>
<body class="nav-md">
    <section id="hero">
        <div class="hero-container">
            <div class="wow fadeIn">
            </div>
        </div>
        <div class="container">
            <h1 class="textoCompleto">
                Sistema de Gestión Vacacional - SGV
            </h1>
            <h1 class="LogoTexto">
                SGV
            </h1>
        </div>
    </section>

    <!-- #header -->
    @Html.Action("Menu", "Principal", new { area = "" })
    <!-- #header -->

    @RenderBody()

    <footer id="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="copyright">
                        &copy; Copyright <strong>Tgestiona</strong>
                    </div>
                </div>
            </div>
        </div>
    </footer><!-- #footer -->

    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

    @Scripts.Render("~/bundles/modernizr/js")
    @Scripts.Render("~/bundles/bootstrap/js")
    @Scripts.Render("~/bundles/main/js")
    @Scripts.Render("~/bundles/Assets/js")
    @Scripts.Render("~/bundles/app/base/js")

    @RenderSection("scripts", required: false)
</body>
</html>
