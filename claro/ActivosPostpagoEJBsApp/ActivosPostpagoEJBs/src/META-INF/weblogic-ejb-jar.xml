<?xml version = '1.0' encoding = 'windows-1252'?>
<weblogic-ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.bea.com/ns/weblogic/weblogic-ejb-jar http://www.bea.com/ns/weblogic/weblogic-ejb-jar/1.0/weblogic-ejb-jar.xsd"
                  xmlns="http://www.bea.com/ns/weblogic/weblogic-ejb-jar">

  <weblogic-enterprise-bean>
    <ejb-name>ActivosPostpagoEJBBean</ejb-name>
    <stateless-session-descriptor>
      <pool>
        <max-beans-in-free-pool>15</max-beans-in-free-pool>
        <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </pool>
      <business-interface-jndi-name-map>
        <business-remote>pe.com.claro.eba.activospostpagoejbs.ejbs.ssbs.ActivosPostpagoEJB</business-remote>
        <jndi-name>ejbActivosPostpagoEjbRemote</jndi-name>
      </business-interface-jndi-name-map>
    </stateless-session-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>60</trans-timeout-seconds>
    </transaction-descriptor>
    <enable-call-by-reference>true</enable-call-by-reference>
  </weblogic-enterprise-bean>

  <weblogic-enterprise-bean>
    <ejb-name>GestionTxEaiMDBBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>25</max-beans-in-free-pool>
        <initial-beans-in-free-pool>15</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>pe.com.claro.jms.activospostpago.eaiTxQueue</destination-jndi-name>
      <connection-factory-jndi-name>pe.com.claro.jms.activospostpago.xa.connfactory</connection-factory-jndi-name>  
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>60</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>GestionVentaCMSMDBBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>20</max-beans-in-free-pool>
        <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>pe.com.claro.jms.activospostpago.cmsVentaQueue</destination-jndi-name>
      <connection-factory-jndi-name>pe.com.claro.jms.activospostpago.xa.connfactory</connection-factory-jndi-name>
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>100</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>GestionPostVentaCMSMDBBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>20</max-beans-in-free-pool>
        <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>pe.com.claro.jms.activospostpago.cmsPostVentaQueue</destination-jndi-name>
      <connection-factory-jndi-name>pe.com.claro.jms.activospostpago.xa.connfactory</connection-factory-jndi-name>  
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>60</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>GestionActualizaEstadosMDBBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>20</max-beans-in-free-pool>
        <initial-beans-in-free-pool>10</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>pe.com.claro.jms.activospostpago.actualizaEstadosQueue</destination-jndi-name>
      <connection-factory-jndi-name>pe.com.claro.jms.activospostpago.xa.connfactory</connection-factory-jndi-name>  
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>60</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>ArchivaTxEaiMDBBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>25</max-beans-in-free-pool>
        <initial-beans-in-free-pool>15</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>pe.com.claro.jms.activospostpago.archivaTxQueue</destination-jndi-name>
      <connection-factory-jndi-name>pe.com.claro.jms.activospostpago.xa.connfactory</connection-factory-jndi-name>
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>60</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>

</weblogic-ejb-jar>
